{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "functions": {
    "backend/api/search/index.ts": {
      "runtime": "@vercel/node"
    },
    "backend/api/metrics/home.ts": {
      "runtime": "@vercel/node"
    },
    "backend/api/dealers/index.ts": {
      "runtime": "@vercel/node"
    },
    "backend/api/employees/index.ts": {
      "runtime": "@vercel/node"
    },
    "backend/api/clients/index.ts": {
      "runtime": "@vercel/node"
    },
    "backend/api/admin/verify-password.ts": {
      "runtime": "@vercel/node"
    },
    "backend/api/admin/verify-totp.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}